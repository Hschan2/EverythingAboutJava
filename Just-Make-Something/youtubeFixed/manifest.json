{
    "manifest_version": 3,
    "name": "유튜브 영상 고정",
    "version": "1.0",
    "permissions": ["activeTab"],
    "icons": {
        "64": "/icons8-fixed-64.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}
